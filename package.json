  "dependencies": {
    "bcryptjs": "^2.4.3",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "mongoose": "^5.13.5",
    "slugify": "^1.6.0",
    "validator": "^13.6.0"
  },
  "devDependencies": {
    "concurrently": "^6.2.0"
